{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <h1>{{ heading_title }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    <div class="container-fluid">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-pencil"></i>{{ text_process }}</h3>
            </div>
            <div class="panel-body">
                <div class="col-md-6">
                    <a href="{{ getStockroom }}"><button type="button"  title="{{ text_button }}" class="btn btn-primary">{{ text_button }}</button></a>
                </div>
            </div>
        </div>
    </div>
</div>
{{ footer }}